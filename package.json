{
  "name": "server",
  "version": "0.0.0",
  "description": "template",
  "repository": "https://github.com/gridaco/serverless-prisma-webpack-nest-server-template",
  "author": "your-name",
  "private": true,
  "license": "MIT",
  "main": "src/main",
  "scripts": {
    "prisma:studio": "npx prisma studio",
    "prisma:generate": "npx prisma generate",
    "prisma:generate:watch": "npx prisma generate --watch",
    "postinstall": "npm run prisma:generate",
    "migrate:dev": "prisma migrate dev",
    "migrate:dev:create": "prisma migrate dev --create-only",
    "migrate:reset": "prisma migrate reset",
    "migrate:deploy": "npx prisma migrate deploy",
    "migrate:status": "npx prisma migrate status",
    "migrate:resolve": "npx prisma migrate resolve",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "setup:dev": "python3 scripts/env_setup.py -e development",
    "setup:prod": "python3 scripts/env_setup.py -e production",
    "start:db": "source prisma/local-dev/start-server.sh",
    "start": "nest start",
    "start:dev": "yarn setup:dev && yarn start:db && nest start --watch",
    "start:debug": "yarn setup:dev && nest start --debug --watch",
    "start:prod": "yarn setup:prod && node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "ngrok": "ngrok http -host-header -region=us -hostname=accounts-services.ngrok.io 4000",
    "deploy:dev": "yarn setup:dev && sls deploy",
    "deploy:prod": "yarn setup:prod && sls deploy --stage production",
    "package:dev": "yarn setup:dev && sls package"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.0.0",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.0.0",
    "@prisma/client": "^2.23.0",
    "aws-lambda": "^1.0.6",
    "aws-serverless-express": "^3.4.0",
    "axios": "^0.21.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "moment": "^2.29.1",
    "nanoid": "^3.1.20",
    "otplib": "^12.0.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "query-string": "^7.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.4"
  },
  "devDependencies": {
    "@hewmen/serverless-plugin-typescript": "^1.1.17",
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@prisma/client": "^2.27.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.3",
    "@types/jest": "26.0.10",
    "@types/node": "^14.14.31",
    "@types/passport": "^1.0.6",
    "@types/passport-jwt": "^3.0.5",
    "@types/request-ip": "^0.0.35",
    "@types/stripe": "^7.13.24",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "copy-webpack-plugin": "^9.0.0",
    "eslint": "7.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "26.4.2",
    "ngrok": "^4.0.1",
    "prettier": "^1.19.1",
    "prisma": "^2.27.0",
    "rimraf": "^3.0.2",
    "serverless": "^2.52.0",
    "serverless-offline": "^7.0.0",
    "serverless-plugin-warmup": "^5.2.3",
    "serverless-webpack": "^5.5.1",
    "supertest": "^4.0.2",
    "ts-jest": "26.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3",
    "webpack": "^5.39.0",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}